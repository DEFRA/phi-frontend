                   {% for item in data.factsheetlinks %}
                <tr class="govuk-table__row">
              
                  <td class="govuk-table__cell"> {{item.PUBLICATION_DATE_FORMATTED}}</td>
                  
                   <td class="govuk-table__cell">  <a class="govuk-link govuk-body" href={{item.DOCUMENT_HYPER_LINK}} >{{item.DOCUMENT_TYPE}} - {{item.TITLE[0]}} </a>
                   <br> ({{item.FILE_EXTENSION}})
                   </td>               
                   
        </tr>
        {% endfor %}
          </tbody>
        </table>
          {% endif %}
