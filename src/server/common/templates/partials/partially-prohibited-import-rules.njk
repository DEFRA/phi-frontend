<h3 class="govuk-heading-s">
    {{data.mainContent.prohibitedHeader}}
</h3>
<div class="govuk-warning-text">
  <span class="govuk-warning-text__icon" aria-hidden="true">!</span>
  <strong class="govuk-warning-text__text">
    <span class="govuk-warning-text__assistive"></span>
    {{data.mainContent.prohibitedContentText1}} {{data.searchQuery.value | capitalize}} {{data.mainContent.importRulesHeaderText2}} {{data.countrySearchQuery.value | capitalize}} {{data.mainContent.prohibitedContentText2}} {{data.format | capitalize}}
   </strong>
   </div> 
<div class="govuk-body">   
 <p class="govuk-body govuk-!-margin-top-0 govuk-!-margin-bottom-1">{{data.mainContent.prohibitedContentText3}} <a class="govuk-link"
            href={{data.url}}>{{data.urlText}}</a></p>

{% if data.result.subformat.length ===1 %}            
    <h3 class="govuk-heading-s">
        {{data.mainContent.exceptionHeader}}
    </h3>
    <div class="govuk-body">   
    <p class="govuk-body govuk-!-margin-top-0 govuk-!-margin-bottom-1">{{data.mainContent.exceptionHelpText}}
        {% if data.result.annex11RulesArr.length > 1 %}
        {% if ulIndicatorFlag %}
            <ul>
                {% if data.result.hybridIndicator.length === 1 %}
                    <li>hybrid</li>
                {% endif %}
                {% if data.result.hybridIndicator.length > 1 %}
                    <li>hybrid({{data.result.hybridIndicator}})</li>
                {% endif %}
                {% if data.result.dormantIndicator.length === 1 %}
                    <li class="govuk-!-margin-bottom-0">dormant</li>
                {% endif %} 
                {% if data.result.dormantIndicator.length > 1 %}
                    <li class="govuk-!-margin-bottom-0">dormant({{data.result.dormantIndicator}})</li>
                {% endif %}
                {% if data.result.seedIndicator.length === 1 %}
                    <li class="govuk-!-margin-bottom-0">seeds</li>
                {% endif %}
                {% if data.result.seedIndicator.length > 1 %}
                    <li class="govuk-!-margin-bottom-0">seeds({{data.result.seedIndicator}})</li>
                {% endif %}
                {% if data.result.fruitIndicator.length === 1 %}
                    <li class="govuk-!-margin-bottom-0">fruit</li>
                {% endif %}
                {% if data.result.fruitIndicator.length > 1 %}
                    <li class="govuk-!-margin-bottom-0">fruit({{data.result.fruitIndicator}})</li>
                {% endif %}
                {% if data.result.bonsaiIndicator.length === 1 %}
                    <li class="govuk-!-margin-bottom-0">naturally and artificially dwarfed</li>
                {% endif %}
                {% if data.result.bonsaiIndicator.length > 1 %}
                    <li class="govuk-!-margin-bottom-0">naturally and artificially dwarfed({{data.result.bonsaiIndicator}})</li>
                {% endif %}
                {% if data.result.invintroIndicator.length === 1 %}
                    <li class="govuk-!-margin-bottom-0">invitro</li>
                {% endif %}
                {% if data.result.invintroIndicator.length > 1 %}
                    <li class="govuk-!-margin-bottom-0">invitro({{data.result.invintroIndicator}})</li>
                {% endif %}
            </ul>
        {% else %}
            {% if data.result.hybridIndicator.length === 1 %}
            <p class="govuk-!-margin-bottom-0">hybrid</p>
            {% endif %}
            {% if data.result.hybridIndicator.length > 1 %}
            <p class="govuk-!-margin-bottom-0">hybrid({{data.result.hybridIndicator}})</p>
            {% endif %}
            {% if data.result.dormantIndicator.length === 1 %}
            <p class="govuk-!-margin-bottom-0">dormant</p>
            {% endif %} 
            {% if data.result.dormantIndicator.length > 1 %}
            <p class="govuk-!-margin-bottom-0">dormant({{data.result.dormantIndicator}})</p>
            {% endif %}
            {% if data.result.seedIndicator.length === 1 %}
            <p class="govuk-!-margin-bottom-0">seeds</p>
            {% endif %}
            {% if data.result.seedIndicator.length > 1 %}
            <p class="govuk-!-margin-bottom-0">seeds({{data.result.seedIndicator}})</p>
            {% endif %}
            {% if data.result.fruitIndicator.length === 1 %}
            <p class="govuk-!-margin-bottom-0">fruit</p>
            {% endif %}
            {% if data.result.fruitIndicator.length > 1 %}
            <p class="govuk-!-margin-bottom-0">fruit({{data.result.fruitIndicator}})</p>
            {% endif %}
            {% if data.result.bonsaiIndicator.length === 1 %}
            <p class="govuk-!-margin-bottom-0">naturally and artificially dwarfed</p>
            {% endif %}
            {% if data.result.bonsaiIndicator.length > 1 %}
            <p class="govuk-!-margin-bottom-0">naturally and artificially dwarfed({{data.result.bonsaiIndicator}})</p>
            {% endif %}
            {% if data.result.invintroIndicator.length === 1 %}
            <p class="govuk-!-margin-bottom-0">invitro</p>
            {% endif %}
            {% if data.result.invintroIndicator.length > 1 %}
            <p class="govuk-!-margin-bottom-0">invitro({{data.result.invintroIndicator}})</p>
            {% endif %}
        {% endif %}
    {% endif %}
    </div>
{% endif %}
{% if data.result.ProhibitionClarification.length > 1 %}
    <h3 class="govuk-heading-s">
        {{data.mainContent.prohibitionClarificationHeader}}
    </h3>
    <p>{{data.result.ProhibitionClarification}}</p>
{% endif %}
</div>

