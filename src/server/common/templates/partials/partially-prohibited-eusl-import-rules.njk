{% if (data.result.annex11RulesArr.length > 0)%}
<h3 class="govuk-heading-s">
    {{data.mainContent.importRulesHeader}}
</h3>
 {% for item in data.result.annex11RulesArr%}
 {% if (item.SERVICE_SUBFORMAT === '')%}
   {% set processedData =  data.removedProcessedData %}
 {% else %}
  {% if (item.SERVICE_SUBFORMAT.toLowerCase() === 'fruit, seeds for eating')%}
   {% set processedData =  'Fruit or Seeds'  %}
 {% endif %}
  {% if (item.SERVICE_SUBFORMAT.toLowerCase() === 'seeds for planting')%}
   {% set processedData =  'Seeds'  %}
 {% endif %}
  {% if (item.SERVICE_SUBFORMAT.toLowerCase() === 'seeds for eating')%}
    {% set  processedData =   'Seeds' %}
  {% endif %}
  {% if (item.SERVICE_SUBFORMAT.toLowerCase() === 'root and tubercle vegetables')%}
    {% set processedData =  'Produce' %}
   {% endif %}
   {% if (item.SERVICE_SUBFORMAT.toLowerCase() === 'fruit') %}
   {% set processedData =  'Fruit' %}
 {% endif %}
 {% if (item.SERVICE_SUBFORMAT.toLowerCase() === 'grain') %}
   {% set processedData =  'Produce' %}
  {% endif %}
   {% if (item.SERVICE_SUBFORMAT.toLowerCase() === 'cut flowers') %}
    {% set processedData =  'Parts of plants' %}
  {% endif %} 
  {% if (item.SERVICE_SUBFORMAT.toLowerCase() === 'wood packaging') %}
    {% set processedData =  'Wood' %}
  {% endif %}
   {% if (item.SERVICE_SUBFORMAT.toLowerCase() === 'cooper products') %}
    {% set processedData =  'Wood' %}
  {% endif %}
 {% endif %}
 
 {% if item['SERVICE_SUBFORMAT_EXCLUDED'].length > 1%}  
    {% set formatHeader = data.format + '( other than ' + item.SERVICE_SUBFORMAT_EXCLUDED +')' %}
  {% else %}
    {% set formatHeader = data.format %}
  {% endif %}
  {% set 11AimportRules = 'false' %}  
{% if ulIndicatorFlag %} 
  {% if item['BTOM_EUSL'] === '11A'%}
  {% set 11AimportRules  = 'true'%}
  {% endif %} 
{% else %}
  {% if item['BTOM_EUSL'] === '11A' and  item['SERVICE_SUBFORMAT'].length > 0 %}
  {% set 11AimportRules = 'true'%}
  {% endif %}
{% endif %}
 {% if 11AimportRules === 'true'%}
    <div class="govuk-body">       
      {% if item['SERVICE_SUBFORMAT_EXCLUDED'].length >= 0 %}  
      {% if item['SERVICE_SUBFORMAT_EXCLUDED'].length === 0%}  
        {% if data.result.hybridIndicator.length >= 1 and processedData.length === 0 %}
        <h3 class="govuk-heading-s">{{formatHeader | capitalize}}</h3>
        {% else %}
        <h3 class="govuk-heading-s">{{processedData}}</h3>
        {% endif %} 
      {% else %}
        {% if ulIndicatorFlag %}
            {% if data.result.hybridIndicator.length >= 1 and processedData.length === 0 %}
            <h3 class="govuk-heading-s">{{item['SERVICE_SUBFORMAT_EXCLUDED'] | capitalize}}</h3>
            {% else %}
             <h3 class="govuk-heading-s"> 
              {{processedData}}
            </h3>
            {% endif %}
       {% else %}
          {% if data.result.hybridIndicator.length >= 1 and processedData.length === 0 %}
            <h3 class="govuk-heading-s">{{item['SERVICE_SUBFORMAT_EXCLUDED'] | capitalize}}</h3>
          {% else %}
